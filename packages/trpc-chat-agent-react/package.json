  "scripts": {
    "build": "rollup --config rollup.config.ts --configPlugin rollup-plugin-swc3",
    "dev": "pnpm build --watch"
  },
  "dependencies": {
    "@preact/signals-core": "^1.8.0",
    "@preact/signals-react": "^3.0.0",
    "@trpc-chat-agent/core": "workspace:*",
    "immer": "^10.1.1",
    "react": "18.2.0",
    "zod": "^3.24.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@types/react": "18.2.43",
    "rollup": "^4.29.1",
    "typescript": "^5.0.0"
