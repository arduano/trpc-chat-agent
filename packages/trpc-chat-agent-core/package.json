{
  "name": "@trpc-chat-agent/core",
  "version": "0.4.10",
  "description": "The core for trpc-chat-agent, an end-to-end framework-agnostic chat toolkit",
  "author": "arduano",
  "license": "MIT",
  "homepage": "https://github.com/arduano/trpc-chat-agent#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arduano/trpc-chat-agent.git"
  },
  "bugs": {
    "url": "https://github.com/arduano/trpc-chat-agent/issues"
  },
  "keywords": [
    "trpc",
    "types",
    "ai",
    "agent",
    "chat"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup --config rollup.config.ts --configPlugin rollup-plugin-swc3",
    "dev": "pnpm build --watch",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix --max-warnings=0"
  },
  "dependencies": {
    "@preact/signals-core": "^1.8.0",
    "@trpc/client": "11.1.0",
    "@trpc/server": "11.1.0",
    "@types/node": "^22.14.1",
    "immer": "^10.1.1",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "rollup": "^4.40.0",
    "typescript": "^5.8.3"
  }
}
